[:project
 [{name me.1/name} xt$column_2]
 [:project
  [me.1/name {xt$column_2 xt$sum_4}]
  [:select
   (< xt$min_3 1930)
   [:group-by
    [me.1/name {xt$sum_4 (sum m.2/length)} {xt$min_3 (min m.2/year)}]
    [:mega-join
     [(= me.1/cert m.2/producer)]
     [[:rename me.1 [:scan {:table MovieExec} [name cert]]]
      [:rename m.2 [:scan {:table Movie} [producer length year]]]]]]]]]
